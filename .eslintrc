{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true
  },
  "globals": {
    "Hull": true
  },
  "rules": {
    "camelcase": [2, { "properties": "never" }],
    "func-names": [0],
    "indent": [2, 2],
    "no-eq-null": [0],
    "no-extra-parens": [0],
    "no-loop-func": [0],
    "no-multi-spaces": [2],
    "no-param-reassign": [0],
    "no-reserved-keys": [2],
    "no-script-url": [0],
    "no-undefined": [2],
    "no-undef-init": [2],
    "no-unused-vars": [2],
    "quotes": [2, "single", "avoid-escape"],
    "react/jsx-uses-react": [2],
    "react/jsx-uses-vars": [2],
    "react/react-in-jsx-scope": [2],
    "strict": [0]
  },
  "plugins": [
    "react"
  ]
}
