{
  "parser": "babel-eslint",
  "globals": {
    "Hull": true
  },
  "rules": {
    "eqeqeq": [2, "smart"],
    "camelcase": [2, { "properties": "never" }],
    "func-names": [0],
    "indent": [2, 2],
    "no-console": [0],
    "no-eq-null": [0],
    "no-extra-parens": [0],
    "no-loop-func": [0],
    "no-multi-spaces": [2],
    "no-param-reassign": [0],
    "id-length": [0],
    "quote-props": [2, "consistent-as-needed"],
    "no-script-url": [0],
    "no-undefined": [2],
    "no-undef-init": [2],
    "no-unused-vars": [2],
    "quotes": [2, "single", "avoid-escape"],
    "react/jsx-uses-react": [2],
    "react/jsx-uses-vars": [2],
    "react/react-in-jsx-scope": [2],
    "strict": [0]
  },
  "extends": "airbnb"
}
